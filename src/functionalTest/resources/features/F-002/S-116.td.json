{
  "title": "must receive an error response when API-Gateway tries to access Get Document Binary Content by Document ID API",

  "_guid_": "S-116",
  "_extends_": "F-002_Test_Data_Base",
  "apiClientId": "ccd_gw",

  "specs": [
    "is to be made on behalf of API-Gateway API"
  ],

  "request": {
    "pathVariables": {
      "documentId": "${[scenarioContext][childContexts][Default_Document_Upload_Data][customValues][documentIdInTheResponse]}"
    }
  },

  "expectedResponse": {
    "_extends_": "Common_403_Response",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "message" : "Access Denied",
      "path" : "/cases/documents/${[scenarioContext][testData][request][pathVariables][documentId]}/binary"
    }
  }
}
